<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.primer_demo.ui.usuario.vistaPerfilControlador">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="200.0" style="-fx-background-color: #154360;">
         <children>
            <Button layoutX="15.0" layoutY="361.0" mnemonicParsing="false" onAction="#volverInicio" prefHeight="25.0" prefWidth="165.0" style="-fx-background-color: white; -fx-background-radius: 50;" text="Volver" textFill="#154360">
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor></Button>
            <Label fx:id="nombretxt" layoutX="23.0" layoutY="28.0" text="Nombre de usuario:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="mailtxt" layoutX="23.0" layoutY="51.0" text="Mail:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="paistxt" layoutX="23.0" layoutY="99.0" text="Pais:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="documentotxt" layoutX="24.0" layoutY="74.0" text="Documento:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="nacimientotxt" layoutX="23.0" layoutY="121.0" text="Nacimiento:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <Label fx:id="vacunadotxt" layoutX="23.0" layoutY="145.0" text="Vacunado:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <Label layoutX="24.0" layoutY="167.0" text="Etiquetas:" textFill="WHITE">
               <font>
                  <Font name="Britannic Bold" size="13.0" />
               </font>
            </Label>
            <VBox fx:id="vbox" layoutX="87.0" layoutY="167.0" prefHeight="100.0" prefWidth="100.0" />
            <Button layoutX="17.0" layoutY="325.0" mnemonicParsing="false" onAction="#modificarPerfil" prefHeight="25.0" prefWidth="165.0" style="-fx-background-color: white; -fx-background-radius: 50;" text="Modificar perfil" textFill="#154360">
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor>
            </Button>
         </children>
      </AnchorPane>
      <TableView fx:id="tabla" layoutX="220.0" layoutY="14.0" prefHeight="206.0" prefWidth="366.0">
        <columns>
          <TableColumn fx:id="columnaReservas" prefWidth="124.0" text="Reservas" />
          <TableColumn fx:id="columnaDireccion" prefWidth="136.0" text="Direccion" />
            <TableColumn fx:id="columnaFecha" prefWidth="105.0" text="Fecha" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
