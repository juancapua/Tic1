<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.primer_demo.ui.operador.operadorControlador">
   <children>
      <AnchorPane layoutY="-1.0" prefHeight="406.0" prefWidth="169.0" style="-fx-background-color: #154360;">
         <children>
            <Button layoutX="13.0" layoutY="14.0" mnemonicParsing="false" onAction="#addDestino" prefHeight="25.0" prefWidth="143.0" style="-fx-background-radius: 50; -fx-background-color: white;" text="Agregar destino" textFill="#154360">
               <cursor>
                  <Cursor fx:constant="CLOSED_HAND" />
               </cursor></Button>
            <Button layoutX="13.0" layoutY="55.0" mnemonicParsing="false" onAction="#bloquearDestino" prefHeight="25.0" prefWidth="143.0" style="-fx-background-radius: 50; -fx-background-color: white;" text="Bloquear destino" textFill="#154360">
