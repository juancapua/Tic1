            <TableColumn fx:id="columnaDetinos" prefWidth="186.0" text="Destino" />
            <TableColumn fx:id="columnaEstado" prefWidth="184.0" text="Estado" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
